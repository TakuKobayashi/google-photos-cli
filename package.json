{
  "name": "google-photos-cli",
  "version": "0.0.1",
  "description": "This is a CLI tool to work with Google Photos",
  "main": "dist/index.js",
  "author": "TakuKobayashi",
  "license": "MIT",
  "bin": {
    "google-photos": "./bin/google-photos-cli.js"
  },
  "scripts": {
    "build": "ts-node scripts/build.ts exec",
    "watch": "ts-node scripts/build.ts exec -w",
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,js}\"",
    "docs": "typedoc --out docs/ && touch docs/.nojekyll",
    "jest": "jest -c jest.config.js",
    "ts-node": "ts-node"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cli-progress": "^3.11.2",
    "commander": "^9.2.0",
    "google-auth-library": "^8.5.1",
    "googlephotos": "^0.3.5",
    "open": "^8.4.0",
    "server-destroy": "^1.0.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/server-destroy": "^1.0.1",
    "documentation": "^14.0.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.15.6",
    "glob": "^8.0.3",
    "jest": "^29.0.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.23.13",
    "typescript": "^4.8.3",
    "typings": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TakuKobayashi/google-photos-cli.git"
  }
}
